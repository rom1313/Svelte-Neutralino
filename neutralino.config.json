{
	"applicationId": "js.neutralino.svelte",
	"version": "1.0.0",
	"defaultMode": "window",
	"port": 13000,
	"documentRoot": "/www/",
	"url": "/",
	"tokenSecurity": "one-time",
	"enableServer": true,
	"enableNativeAPI": true,
	"logging": {
		"enabled": true,
		"writeToLogFile": true
	},
	"nativeBlockList": [],
	"modes": {
		"window": {
			"title": "Svelte Neutralino App",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"fullScreen": true,
			"alwaysOnTop": false,
			"icon": "/www/icons/appIcon.png",
			"enableInspector": true,
			"borderless": false,
			"maximize": false,
			"hidden": false,
			"resizable": true,
			"exitProcessOnClose": false
		},
		"browser": {
			"globalVariables": {},
			"nativeBlockList": [
				"filesystem.*"
			]
		}
	},
	"cli": {
		"binaryName": "svelte-neutralino",
		"resourcesPath": "/www/",
		"extensionsPath": "/extensions/",
		"clientLibrary": "/www/js/neutralino.js",
		"binaryVersion": "4.4.0",
		"clientVersion": "3.3.0"
	}
}